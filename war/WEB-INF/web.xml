<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Oracle Corporation//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>edu.cmu.mixer.wrapup.HelloWorld</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>accesstasks</servlet-name>
    <servlet-class>edu.cmu.mixer.access.AccessTaskList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>accesslog</servlet-name>
    <servlet-class>edu.cmu.mixer.access.AccessLog</servlet-class>
  </servlet>
  <!--
  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  -->
  <error-page>
    <error-code>404</error-code>
    <location>/404.html</location>
  </error-page>
  <servlet-mapping>
    <servlet-name>accesstasks</servlet-name>
    <url-pattern>/access/tasklist.csv</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>accesslog</servlet-name>
    <url-pattern>/access/log</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>feedback</servlet-name>
    <servlet-class>edu.cmu.mixer.wrapup.WrapperFeedback</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>feedback</servlet-name>
    <url-pattern>/feedback</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>query</servlet-name>
    <servlet-class>edu.cmu.mixer.wrapup.WrapupQuery</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>query</servlet-name>
    <url-pattern>/query</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addwrapper</servlet-name>
    <servlet-class>edu.cmu.mixer.wrapup.AddWrapper</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addwrapper</servlet-name>
    <url-pattern>/admin/wrapper/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addorder</servlet-name>
    <servlet-class>edu.cmu.mixer.access.AddOrdering</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addorder</servlet-name>
    <url-pattern>/admin/ordering/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>bulk</servlet-name>
    <servlet-class>edu.cmu.mixer.util.BulkExport</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bulk</servlet-name>
    <url-pattern>/admin/bulk</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>bulk</servlet-name>
    <url-pattern>/admin/bulk/*</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>main.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <display-name>Remote API Servlet</display-name>
    <servlet-name>RemoteApiServlet</servlet-name>
    <servlet-class>com.google.apphosting.utils.remoteapi.RemoteApiServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>RemoteApiServlet</servlet-name>
    <url-pattern>/remote_api</url-pattern>
  </servlet-mapping>
</web-app>
